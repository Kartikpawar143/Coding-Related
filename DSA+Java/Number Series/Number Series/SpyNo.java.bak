class SpyNo 
{
	public static void main(String[] args) 
	{
		int num=1234;
		int sum=0;
		int mul=1;
		int num1 = num;

		while (num>0)
		{
			int rem=num%10;
			mul=mul*rem;
			sum=sum+rem;
			num=num/10;
		}
		if (sum==mul)
		{
			System.out.println(num1 +" Is Spy No.");
		}
		else 
			System.out.println(num1 + " Is Not a Spy No.");
	}
}
