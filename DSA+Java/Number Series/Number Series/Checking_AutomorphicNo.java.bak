class Checking_AutomorphicNo 
{
	public static void main(String[] args) 
	{
		int num=76;
		int count=0;
		int res=num;

		while (num>0)
		{
			count++;
			num/=10;
		}
		num=res;
		int den=10;
		int square=num*num;
		for (int i=1;i<=count;i++ )
		{
			den=den*10;
		}
		int rem=square%den;
		if (rem==num)
		{
			System.out.println(num+ " Is Automorphic No.");
		}
		else
			System.out.println(num+ " Is Automorphic No.");
	}
}
